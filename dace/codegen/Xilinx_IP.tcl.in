open_project ip_cores
set_top ${DACE_KERNEL_NAME}
add_files -cflags "${DACE_XILINX_SYNTHESIS_FLAGS} -I${DACE_RUNTIME_DIR}/include -I${DACE_HLSLIB_DIR}/include -I${CMAKE_BINARY_DIR}" "${DACE_IP}"
open_solution "${DACE_KERNEL_NAME}" -flow_target vivado
